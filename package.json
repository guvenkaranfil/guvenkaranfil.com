    "@next/mdx": "^9.5.3",
    "axios": "^0.21.1",
    "date-fns": "^2.16.1",
    "framer-motion": "^3.2.0",
    "fuse.js": "3.4.3",
    "gray-matter": "^4.0.2",
    "lodash.debounce": "^4.0.8",
    "lodash.groupby": "^4.6.0",
    "mdx-prism": "^0.3.1",
    "next": "10.0.4",
    "next-images": "^1.4.1",
